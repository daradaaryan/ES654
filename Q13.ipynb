    "import numpy as np\n",
    "\n",
    "def argmin(arr):\n",
    "  min = arr[0]\n",
    "  mini = 0\n",
    "\n",
    "  for i in range(len(arr)):\n",
    "    if arr[i] < min:\n",
    "      min = arr[i]\n",
    "      mini = i\n",
    "  return mini\n",
    "\n",
    "arr=np.array([5,6,8,3,13,6,4,2,1,3])\n",
    "print(argmin(arr))\n",
    "print(np.argmin(arr))"
   ]
  }
